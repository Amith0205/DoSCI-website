<section class="important_dates">
  <div class="container">
    <div class="row">


  <div class="col m12 s12 l12">
    <div class="card-panel ">
      <span class="">
        <b>Important Dates</b>
        <hr />


     <% @important_dates.each do |impdate| %>
     <span><%= impdate.title %></span>
     <br>
      <%= impdate.date.to_date.to_formatted_s(:long_ordinal) %>
      <br>
     <hr />

     <% end %>

   </span>
 </div>
 </div>
 </div>
 </div>
</section>
